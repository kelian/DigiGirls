<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.digigirls.habitapp">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".vaated.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".vaated.AddActivity"
            android:label="@string/lisa_harjumus"
            android:parentActivityName=".vaated.MainActivity"/>
        <activity android:name=".vaated.SettingsActivity"
            android:label="@string/litsentsid"
            android:parentActivityName=".vaated.MainActivity"/>
    </application>

</manifest>
