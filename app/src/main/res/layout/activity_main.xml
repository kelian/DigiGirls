<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.digigirls.habitapp.vaated.MainActivity">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clipToPadding="false"
        android:paddingBottom="@dimen/main_list_padding_bottom"
        android:paddingLeft="@dimen/main_list_padding"
        android:paddingRight="@dimen/main_list_padding" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/lisaNupp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/general_padding"
        android:src="@drawable/ic_add"
        app:fabSize="normal" />

</FrameLayout>
